#include <iostream>
using namespace std;

int main() {
	int n, sum=0, x;
	cout<<"Hello, How many elements do you have?"<<endl;
	cin>>n;
	
	for(int i =0; i<n; i++){
		x=i+1;
		cout<<"Enter the element at "<<x<<endl;
		int arr[n];
		cin>>arr[i];
		
		sum+=arr[i];
	}
	cout<<"The sum of the numbers you entered is: "<<sum<<endl;
	
	
	
	return 0;
}
